        <activity android:name=".activities.YEAR" />
        <activity android:name=".activities.Admin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Student_main_2"></activity>
    </application>

</manifest>